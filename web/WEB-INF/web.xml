    <context-param>
        <param-name>userContext</param-name>
        <param-value>sa</param-value>
    </context-param>
    <context-param>
        <param-name>passContext</param-name>
        <param-value>12345678</param-value>
    </context-param>
    <servlet>
        <servlet-name>loadDbSe1625</servlet-name>
        <servlet-class>Controllers.loadDB_SE1625</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update</servlet-name>
        <servlet-class>Controllers.Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadPage</servlet-name>
        <servlet-class>Controllers.LoadPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controllers.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loadDbSe1625</servlet-name>
        <url-pattern>/loadDbSe1625</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update</servlet-name>
        <url-pattern>/Update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadPage</servlet-name>
        <url-pattern>/LoadPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
